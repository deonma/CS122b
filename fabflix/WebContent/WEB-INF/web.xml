    <servlet-name>employee_login.jsp</servlet-name>
    <url-pattern>/_dashboard/*</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Dashboard</servlet-name>
    <url-pattern>/Dashboard</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>searchAutoComplete</servlet-name>
    <url-pattern>/searchAutoComplete</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GenreDropDown</servlet-name>
    <url-pattern>/GenreDropDown</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Popup</servlet-name>
    <url-pattern>/Popup</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MobileLogin</servlet-name>
  	<url-pattern>/MobileLogin</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>MobileHome</servlet-name>
  	<url-pattern>/MobileHome</url-pattern>
  </servlet-mapping>
  
  <welcome-file-list>
    <welcome-file>login.jsp</welcome-file>
  </welcome-file-list>
 
